{
  "app_location": "/",
  "api_location": "api",
  "output_location": "dist",
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  },
  "responseOverrides": {
    "404": { "rewrite": "/index.html" }
  },
  "mimeTypes": {
    ".css": "text/css",
    ".js": "application/javascript",
    ".html": "text/html"
  }
}
