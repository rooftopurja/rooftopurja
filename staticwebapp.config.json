{
  "routes": [
    {
      "route": "/login.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/SendOtp",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/VerifyOtp",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/.auth/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  }
}
