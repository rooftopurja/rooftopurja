{
  "navigationFallback": {
    "rewrite": "/login.html",
    "exclude": [
      "/api/*",
      "/.auth/*",
      "/favicon.ico"
    ]
  },
  "routes": [
    {
      "route": "/login.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/favicon.ico",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/SendOtp",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/VerifyOtp",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ]
}
