{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/GetPremier300MeterAll",
      "redirect": "/api/GetPremier300MeterAll",
      "statusCode": 301
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
